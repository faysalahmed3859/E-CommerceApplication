@model IEnumerable<Handy.Models.AutoGenerateSerialNumber>
@{
    ViewData["Title"] = "Auto Serial Number";
}


<div class="action">
    <div class="row">
        <div class="col-md-3 col-12 col-lg-3">
            <a class="create" onclick="ShowInSmallPopup('@Url.Action("AddOrEdit","AutoGenerateSerialNumbers",null,Context.Request.Scheme)','New AutoGenerateSerialNumber' )"><i class="fas fa-plus"></i><span>New</span></a>
            <p class="recordcount">@Model.Count() record exist</p>
        </div>
    </div>
</div>
<div class="body">
    <div class="row">
        <div class="col-md-12 col-12 col-lg-12">
            <div id="view-all">
                @await Html.PartialAsync("_ViewAllAutoGenerateSerialNumber", Model)
            </div>
        </div>
    </div>
</div>